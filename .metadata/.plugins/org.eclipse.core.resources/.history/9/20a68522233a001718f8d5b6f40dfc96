package com.fqy.may;

public class Sync {
	public static void main(String[] args) {
	}
}

class Counter implements Runnable{
	Example example = new Example();
	
	@Override
	public void run() {
		// TODO Auto-generated method stub
		System.out.println(example.getNextValue());
	}
	
}
class Example {
	private int value = 0;

	public int getNextValue() {
		return value++;
	}

}